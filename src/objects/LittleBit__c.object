<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>Hierarchy</customSettingsType>
    <customSettingsVisibility>Public</customSettingsVisibility>
    <description>This custom setting stores the device Id and access token for a littleBits device in a hierarchy allowing multiple devices per org and per user group.</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>accessToken__c</fullName>
        <description>The access token for the device we are working with.</description>
        <externalId>false</externalId>
        <label>accessToken</label>
        <length>64</length>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>deviceId__c</fullName>
        <description>The Id of the device we are working with.</description>
        <externalId>false</externalId>
        <label>deviceId</label>
        <length>12</length>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <label>LittleBits Information</label>
</CustomObject>
